<launch>
    <!--  Can't pass empty string-->
    <arg name="ID" default =""/>
    <!-- joy node -->
    <node pkg="joy" type="joy_node" name="joy_node$(arg ID)"/>
    <!-- driving agv -->
    <node pkg="tircgo_uart" type = "tircgo_uart_encoder_decoder_node" name = "tircgo_encoder_decoder_nodename" output="screen"/>
    <!-- brain -->
    <node pkg="tircgo_controller" type="tircgo_controller_node" name="tircgo_controller_node$(arg ID)" args = "$(arg ID)" output="screen"/>
</launch>